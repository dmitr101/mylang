<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<scanner>
	<keywords>
		<keyword name = "module"/>
		<keyword name = "if"/>
		<keyword name = "else"/>
		<keyword name = "do"/>
		<keyword name = "while"/>
		<keyword name = "end"/>
		<keyword name = "read"/>
		<keyword name = "write"/>
	</keywords>
	<classes>
		<!--General characters-->
		<class name = "digit" rexp = "\d"/>
		<class name = "letter" rexp = "[A-Z]|[a-z]"/>
		<class name = "eq" rexp = "[=]"/>
		<!--Arythmetic characters-->
		<class name = "add" rexp = "[+]"/>
		<class name = "sub" rexp = "[-]"/>
		<class name = "mul" rexp = "[*]"/>
		<class name = "div" rexp = "[/]"/>
		<!--Comparison characters-->
		<class name = "less" rexp = "<"/>
		<class name = "greater" rexp = ">"/>
		<!--Delim characters-->
		<class name = "dot" rexp = "[.]"/>
		<class name = "comma" rexp = "[,]"/>
		<class name = "col" rexp = "[:]"/>
		<class name = "scol" rexp = "[;]"/>
		<class name = "obr" rexp = "[(]"/>
		<class name = "cbr" rexp = "[)]"/>
	</classes>
	<states>
		<state name = "default" type = "state_default">
			<transition class = "digit" state = ""/>
		</state>
		
	</states>
</scanner>